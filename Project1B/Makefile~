default:
	gcc -g -Wall -Wextra -o -lz lab1b-client lab1b-client.c
	gcc -g -Wall -Wextra -o -lz lab1b-server lab1b-server.c

client:
	gcc -g -Wall -Wextra -o -lz lab1b-client lab1b-client.c

server:
	gcc -g -Wall -Wextra -o -lz lab1b-server lab1b-server.c

